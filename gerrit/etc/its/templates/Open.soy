{namespace etc.its.templates}

/**
 * @param changeNumber
 * @param changeUrl
 * @param ownerUsername
 * @param subject
 * @param formatChangeUrl
 */
{template .Open autoescape="strict" kind="text"}
  Issue had a related patch set [{$changeNumber}|{$changeUrl}] uploaded by [~{$ownerUsername}]:{\n}
  {\n}
  {$subject}{\n}
  {\n}
  {$formatChangeUrl}
{/template}
